{
  "manifest_version": 3,
  "name": "Code inject MV3",
  "options_page":  "options.html",
  "version": "2025.1",
  "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage","unlimitedStorage"],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Code inject MV3"
  },
  	"icons": {
        "128": "icon.png"
    }
}